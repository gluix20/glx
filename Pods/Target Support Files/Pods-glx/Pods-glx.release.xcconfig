GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AeroGearHttp" "${PODS_ROOT}/Headers/Public/AeroGearOAuth2"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AeroGearHttp" -isystem "${PODS_ROOT}/Headers/Public/AeroGearOAuth2"
OTHER_LDFLAGS = -ObjC -l"Pods-glx-AeroGearHttp" -l"Pods-glx-AeroGearOAuth2" -framework "Security"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods